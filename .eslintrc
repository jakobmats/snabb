{
  "rules": {
    "space-in-parens": 0,
    "array-bracket-spacing": 0,
    "camelcase": 0,
    "curly": 0,
    "quotes": 0,
    "no-undef": 2,
    "semi": [
      2,
      "always"
    ],
    "yoda": [
      2,
      "always",
      {
        "onlyEquality": true
      }
    ],
    "operator-linebreak": 0,
    "valid-jsdoc": [
      2,
      {
        "requireReturn": false,
        "requireReturnDescription": false,
        "requireParamDescription": false
      }
    ],
    "indent": [
      "error",
      "tab"
    ],
    "consistent-this": [
      2, "that"
    ],
    "max-len": [
      1,
      150,
      {
        "tabWidth": 1
      }
    ],
    "no-multi-str": 2
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "jquery": true
  },
  "globals": {
    "localStorage": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  }
}
